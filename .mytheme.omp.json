{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "version": 2,
    "final_space": true,
    "console_title_template": " {{ .Folder }} :: {{if .Root}}Admin{{end}}",
    "terminal_background": "p:t-background",
    "blocks": [
        {
            "type": "prompt",
            "alignment": "left",
            "segments": [
                {
                    "type": "iterm",
                    "interactive": true,
                    "style": "plain",
                    "foreground": "#80ffea",
                    "template": "{{ .PromptMark }}"
                },
                {
                    "type": "path",
                    "style": "plain",
                    "foreground": "p:pistachio-green",
                    "template": "<b>{{ .Path }}</b> <p:pistachio-green>⚡</>",
                    "properties": {
                        "style": "folder"
                    }
                },
                {
                    "type": "git",
                    "style": "plain",
                    "foreground": "p:light-sky-blue",
                    "foreground_templates": [
                        "{{ if or (.Working.Changed) (.Staging.Changed) }}p:terminal-red{{ end }}",
                        "{{ if and (gt .Ahead 0) (gt .Behind 0)}}p:light-sky-blue {{ end }}",
                        "{{ if gt .Ahead 0 }}p:terminal-blue{{ end }}",
                        "{{ if gt .Behind 0 }}p:celeste-blue{{ end }}"
                    ],
                    "template": "({{ .HEAD}})",
                    "properties": {
                        "branch_icon": "\ue725 ",
                        "fetch_status": true
                    }
                },
                {
                    "type": "status",
                    "style": "plain",
                    "foreground": "p:terminal-red",
                    "template": " \uf00d"
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "right",
            "segments": [
                {
                    "type": "rust",
                    "style": "plain",
                    "foreground": "#193549",
                    "background": "#99908a",
                    "template": "  {{ .Full }} "
                },
                {
                    "type": "lua",
                    "style": "plain",
                    "foreground": "white",
                    "template": "  {{ .Major }} "
                },
                {
                    "type": "node",
                    "style": "plain",
                    "foreground": "p:pistachio-green",
                    "template": "\ue718 {{ .Full }} "
                },
                {
                    "type": "php",
                    "style": "plain",
                    "foreground": "p:terminal-blue",
                    "template": "\ue73d {{ .Full }} "
                },
                {
                    "type": "python",
                    "style": "plain",
                    "foreground": "p:terminal-yellow",
                    "template": "\ue235 {{ .Full }}"
                },
                {
                    "type": "julia",
                    "style": "plain",
                    "foreground": "p:terminal-magenta",
                    "template": "\ue624 {{ .Full }}"
                },
                {
                    "type": "ruby",
                    "style": "plain",
                    "foreground": "p:terminal-red",
                    "template": "\ue791 {{ .Full}}"
                },
                {
                    "type": "go",
                    "style": "plain",
                    "foreground": "p:light-sky-blue",
                    "template": "\ufcd1 {{ .Full}}"
                },
                {
                    "type": "command",
                    "style": "plain",
                    "foreground": "p:white-blue",
                    "properties": {
                        "command": "git log --pretty=format:%cr -1 ||",
                        "shell": "alacritty"
                    }
                },
                {
                    "type": "time",
                    "style": "plain",
                    "foreground": "p:pistachio-green",
                    "properties": {
                        "time_format": "15:04:05"
                    }
                }
            ],
            "overflow": "hide"
        },
        {
            "type": "prompt",
            "alignment": "left",
            "segments": [
                {
                    "type": "text",
                    "style": "plain",
                    "foreground": "p:pistachio-green",
                    "template": "\u276f"
                }
            ],
            "newline": true
        }
    ],
    "transient_prompt": {
        "foreground": "p:terminal-blue",
        "background": "p:t-background",
        "template": "\u276f"
    },
    "secondary_prompt": {
        "foreground": "p:terminal-blue",
        "background": "transparent",
        "template": "\u276f"
    },
    "palette": {
        "blue-bell": "#9aa5ce",
        "blue-black": "#565f89",
        "celeste-blue": "#b4f9f8",
        "light-sky-blue": "#7dcfff",
        "main-bg": "#24283b",
        "pastal-grey": "#cfc9c2",
        "pistachio-green": "#9ece6a",
        "t-background": "p:main-bg",
        "terminal-black": "#414868",
        "terminal-blue": "#7aa2f7",
        "terminal-green": "#73daca",
        "terminal-magenta": "#bb9af7",
        "terminal-red": "#f7768e",
        "terminal-white": "#c0caf5",
        "terminal-yellow": "#e0af68",
        "white-blue": "#a9b1d6"
    }
}
